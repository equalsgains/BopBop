{
    "name": "BopBop by Luis",
    "manifest_version": 2,
    "version": "0.1.0",
    "description": "Used to gather information from the course",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [ {
    "js": [ "jquery-2.1.3.min.js", "popup.js"],
    "matches": [ "http://*/*", "https://*/*"]
    }],

    "permissions": [
        "activeTab", "http://*/*", "https://*/*", "storage"
    ]
}